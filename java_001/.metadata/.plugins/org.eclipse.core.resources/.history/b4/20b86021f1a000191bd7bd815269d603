import java.util.*;

public class KeepingStats {
	
	public static void main(String[] args) {
  	
	int heads = 1;
  	int tails = 2;
  	int toss = (int)Math.floor((Math.random() * ((tails - heads) + 1)) + heads);
  	//make tally an array
  	//int tally = 0;
  	
  	int[] tally = {25, 2};
  	
  	
  	//testing output
  	System.out.println("100 Coin Flips");
  	System.out.println("Count of Head: " + tally[1]);
  	System.out.println("Count of Tail: " + tally[2]);
  	//System.out.println(toss);
  }
}
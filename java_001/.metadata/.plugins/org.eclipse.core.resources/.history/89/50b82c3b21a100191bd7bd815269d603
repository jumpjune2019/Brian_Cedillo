		String [] colDie = {"Die 1","Die 2","Die 3","Die 4","Die 5","Die 6"};
		String [] rownum = {"Die 1","Die 2","Die 3","Die 4","Die 5","Die 6"};
		//System.out.println(Arrays.toString(colDie));
		for (int row = 0; row < rownum.length; row++) {
		System.out.print(String.format("%20s",colDie[row]));
		}
		System.out.println("");
		
		for (int row = 0; row < rownum.length; row++) {
            System.out.print(String.format("%-9s ",rownum[row]));
            {
		for (int col = 0; col < labels[row].length; col++) {
			System.out.print(String.format("%-18s ",labels[row][col]));
		}
		System.out.println();
	}}
	
		/*
